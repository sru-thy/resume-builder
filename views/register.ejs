<h1>Sign Up Form</h1>
<form action="/register" method="POST">
    <% if (message != undefined) { %>
         <h2> <%= message %> </h2>
        <% } %>
    <input type="text" name="username" placeholder="username">
    <input type="password" name="password" placeholder="password">
    <button>Submit</button>
</form>